[
  {
    "key": "tab",
    "command": "selectNextSuggestion",
    "when": "suggestWidgetVisible && suggestWidgetHasFocusedSuggestion"
  },
  {
    "key": "tab",
    "command": "tab",
    "when": "editorTextFocus && !suggestWidgetVisible && !editorReadonly && !hasOtherSuggestions"
  },
  {
    "key": "shift+tab",
    "command": "selectPrevSuggestion",
    "when": "suggestWidgetVisible && suggestWidgetHasFocusedSuggestion"
  }
]
